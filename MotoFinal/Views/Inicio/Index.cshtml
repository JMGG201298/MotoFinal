
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutMenu.cshtml";
}
@section estilos{
    <link rel="stylesheet" href="~/Contenido/estilos/InicioCss.css" />
}
<div class="container-xl">
    <div class="row">
        <div class="col-xl-12">
            <h5>Categorias</h5>
        </div>
    </div>
    <div class="container-xl conti">
        <div class="row" id="fila-contenedor">
            <div id="principalCarta" data-value="0" class="col-2 col-md-1">
                <div class="card card-propio">
                    <img src="~/Contenido/imagenes/5936.jpg" />
                    <div class="card-body card-body-propio">
                        <p>Todos</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="container-xl">
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="~/Contenido/imagenes/5936.jpg" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="~/Contenido/imagenes/5936.jpg" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="~/Contenido/imagenes/5936.jpg" class="d-block w-100" alt="...">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>
@section Scripts{
    <script src="~/Contenido/Javascript/InicioJs.js"></script>
}